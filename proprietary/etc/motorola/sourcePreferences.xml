<list>
	<!-- EACH MAP IN THIS LIST DEFINES SETTINGS FOR account of that type. Please ensure that there are no duplicate entries for same type as this may lead to cloberring of these settings, the later settings would overide the preceding setting in that case -->

	<map>
		<!-- Settings for account type "com.google" -->
		<string name="accountType">com.google</string>

		<!-- Pretty name is constant string value "Google" -->
		<string name="prettyName">strVal:Google</string>

		<!-- Secondary Name is accountName -->
		<string name="secondaryName">refVal:accountName</string>

		<!-- This account type allows out of context pictures to be displayed for this contact -->
		<string name="allowOutOfContext">intVal:1</string>

		<!-- This account is editable in blur contacts app -->
		<string name="activeSyncSource">intVal:1</string>

		<!-- This Account Type does not have blur source name associated with it -->
		<string name="blurSourceName">strVal:</string>

		<!-- This account type creates contacts of type "contacts" (In blur contacts can be either "contacts" or "friends") -->
		<string name="capability">capabilityVal:contacts</string>

		<!-- This account type is not part of Picture algorithm preferrd picture source selection -->
		<string name="pictureSource">intVal:0</string>
	</map>

